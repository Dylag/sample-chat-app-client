<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="800.0" maxWidth="400" prefHeight="800.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testjavafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <GridPane maxHeight="816.0" maxWidth="533.0" prefHeight="816.0" prefWidth="463.0">
     <columnConstraints>
       <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" percentWidth="25.0" />
       <ColumnConstraints hgrow="SOMETIMES" percentWidth="25.0" />
         <ColumnConstraints hgrow="SOMETIMES" percentWidth="25.0" />
         <ColumnConstraints hgrow="SOMETIMES" percentWidth="25.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints maxHeight="147.0" minHeight="0.0" percentHeight="8.0" prefHeight="51.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="465.0" minHeight="0.0" percentHeight="8.0" prefHeight="62.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="546.0" minHeight="0.0" percentHeight="8.0" prefHeight="48.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="643.0" minHeight="10.0" prefHeight="567.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="643.0" minHeight="10.0" prefHeight="567.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" percentHeight="8.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Button mnemonicParsing="false" onAction="#onConnectToServerClick" prefHeight="44.0" prefWidth="400.0" text="connect to server" GridPane.columnSpan="4" GridPane.rowIndex="1">
            <font>
               <Font size="15.0" />
            </font></Button>
         <Button mnemonicParsing="false" onAction="#setNickname" prefHeight="52.0" prefWidth="100.0" text="set nick" GridPane.columnIndex="3" GridPane.rowIndex="2" />
         <TextArea fx:id="chatArea" prefHeight="194.0" prefWidth="400.0" GridPane.columnSpan="4" GridPane.rowIndex="3" GridPane.rowSpan="2">
            <font>
               <Font size="14.0" />
            </font></TextArea>
         <TextField fx:id="messageTextField" onAction="#sendMessage" prefHeight="25.0" prefWidth="300.0" promptText="message" GridPane.columnSpan="3" GridPane.rowIndex="5">
            <font>
               <Font size="18.0" />
            </font></TextField>
         <Button mnemonicParsing="false" onAction="#sendMessage" prefHeight="36.0" prefWidth="100.0" text="send!" GridPane.columnIndex="3" GridPane.rowIndex="5" />
         <TextField fx:id="serverIPTextField" prefHeight="39.0" prefWidth="200.0" promptText="server ip" text="192.168.0.14" GridPane.columnSpan="2">
            <font>
               <Font size="18.0" />
            </font>
         </TextField>
         <TextField fx:id="serverPortTextField" minHeight="-Infinity" minWidth="-Infinity" prefHeight="39.0" prefWidth="169.0" promptText="server port" text="6040" GridPane.columnIndex="2" GridPane.columnSpan="2">
            <font>
               <Font size="18.0" />
            </font>
         </TextField>
         <TextField fx:id="nicknameField" onAction="#setNickname" prefHeight="50.0" prefWidth="300.0" promptText="nickname" GridPane.columnSpan="3" GridPane.rowIndex="2">
            <font>
               <Font size="18.0" />
            </font>
         </TextField>
      </children>
      <VBox.margin>
         <Insets bottom="-10.0" left="-10.0" right="-10.0" top="-10.0" />
      </VBox.margin>
   </GridPane>
</VBox>
